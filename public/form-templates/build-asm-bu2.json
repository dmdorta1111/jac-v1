{
  "formId": "build-asm-bu2",
  "itemType": "custom",
  "title": "Build Asm Bu2",
  "description": "Configuration parameters for build asm bu2",
  "sections": [
    {
      "id": "build-asm-bu2-configuration",
      "title": "Configuration",
      "fields": [
        {
          "id": "build-asm-bu2-frame-wo",
          "name": "FRAME_WO",
          "label": "Frame Wo",
          "required": false,
          "type": "input",
          "inputType": "text",
          "placeholder": "",
          "conditional": {
            "conditions": [
              {
                "field": "FRAMES_",
                "operator": "equals",
                "value": 1
              }
            ],
            "logic": "AND"
          }
        },
        {
          "id": "build-asm-bu2-door-wo",
          "name": "DOOR_WO",
          "label": "Door Wo",
          "required": false,
          "type": "input",
          "inputType": "text",
          "placeholder": "",
          "conditional": {
            "conditions": [
              {
                "field": "DOORS_",
                "operator": "equals",
                "value": 1
              }
            ],
            "logic": "AND"
          }
        },
        {
          "id": "build-asm-bu2-frames",
          "name": "FRAMES_",
          "label": "Frames",
          "type": "switch",
          "defaultValue": false,
          "helperText": "Toggle to enable"
        },
        {
          "id": "build-asm-bu2-doors",
          "name": "DOORS_",
          "label": "Doors",
          "type": "switch",
          "defaultValue": false,
          "helperText": "Toggle to enable"
        }
      ]
    }
  ],
  "submitButton": {
    "text": "Save Configuration",
    "action": "save-config"
  }
}