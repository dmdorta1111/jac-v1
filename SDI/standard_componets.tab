		!CREATE STOCK REQUISITION

		SHEET_INDEX = TOTAL_SHEETS
		MAX_LINES = 26

		IF ITEM_INDEX <= ITP AND STOCKTYPE == 3
			WHILE ITEM_INDEX <= ITP AND STOCKTYPE == 3
				SHEET_INDEX++
