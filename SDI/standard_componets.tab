IF DRAWINGS == 1
	COMP_ROW = 6	
	EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "COMPONENTS"
	EXCEL_GET_VALUE COMP_ROW 0 ITEM_NUM
	EXCEL_GET_VALUE COMP_ROW 1 SUB_TYPE
	EXCEL_GET_VALUE COMP_ROW 2 QTY
	DECLARE_ARRAY DRWS

	READ_DIRECTORY PROJECT_DIRECTORY "*.drw" DRW_FILES
	GET_ARRAY_SIZE DRW_FILES FILE_SIZE

	IF ITEM_NUM == ""
		EOF = TRUE
	ELSE
		EOF = FALSE
	END_IF
	
	DECLARE_VARIABLE STRING DRW_NAME
	DECLARE_VARIABLE STRING GENERIC_DRW_NAME
	DECLARE_VARIABLE STRING INST_DRW_NAME

	WHILE EOF == FALSE
		IF QTY > 0
			IF SUB_TYPE == "ASSEMBLY" OR SUB_TYPE == "ASM"
				RETRIEVE_MDL ITEM_NUM+".ASM" ITEM
			ELSE_IF SUB_TYPE == "PART"
				RETRIEVE_MDL ITEM_NUM+".PRT" ITEM
			END_IF
			! REGEN_MDL ITEM FORCE
			GET_REF_GENERIC ITEM GENERIC
			GET_REF_NAME GENERIC GENERIC_NAME
			IF ITEM_NUM == GENERIC_NAME
				DRW_NAME = ITEM_NUM+".DRW"
				DRW_NAME = mklower(DRW_NAME)
			ELSE
				GENERIC_DRW_NAME = GENERIC_NAME+".DRW"
				GENERIC_DRW_NAME = mklower(GENERIC_DRW_NAME)
				INST_DRW_NAME = ITEM_NUM+".DRW"				
				INST_DRW_NAME = mklower(INST_DRW_NAME)

				FIND_ARRAY_ELEM DRW_FILES INST_DRW_NAME DRW_ELEM
				IF DRW_ELEM >= 0
					DRW_NAME = INST_DRW_NAME
				ELSE
					DRW_NAME = GENERIC_DRW_NAME
				END_IF
			END_IF
			
			FIND_ARRAY_ELEM DRWS DRW_NAME ARRAY_ELEM
			IF ARRAY_ELEM == -1
				FIND_ARRAY_ELEM DRW_FILES DRW_NAME DRW_ELEM
				IF DRW_ELEM >= 0
					ADD_ARRAY_ELEM DRWS DRW_NAME
				END_IF
			END_IF
		END_IF
		
		COMP_ROW++
		EXCEL_GET_VALUE COMP_ROW 0 ITEM_NUM
		EXCEL_GET_VALUE COMP_ROW 1 SUB_TYPE
		EXCEL_GET_VALUE COMP_ROW 2 QTY
		
		IF ITEM_NUM == ""
			EOF = TRUE
		ELSE
			EOF = FALSE
		END_IF
	END_WHILE
	
	GET_ARRAY_SIZE DRWS sizeDRWS

	IF sizeDRWS > 0
		FOR DRW REF ARRAY DRWS
			FIND_ARRAY_ELEM DRWS DRW INDEX
			IF INDEX >= 0
				RETRIEVE_MDL &DRW DRW				
				GET_MDL_NAME DRW name
				REGEN_MDL DRW FORCE
				SET_WORKING_DIRECTORY DXF_DIRECTORY					
				DECLARE_STRUCT PDF_OPTION option
				option.title = "sample drawing"
				option.author = "DRAFTER"
				option.subject = "detail"
				option.keywords = "SmartAssembly example"
				option.launch_viewer = "FALSE"
				option.color_depth = "Gray"
				EXPORT_DRW_PDF DRW option &name
				SET_WORKING_DIRECTORY PROJECT_DIRECTORY	
				ERASE_NOT_DISPLAYED
			END_IF
		END_FOR
	END_IF
	CLEAR_ARRAY DRWS
END_IF

!RESETS THE USED PARAMETER IN EACH COMPONENT TO  USED = 0 - ALSO  SETS CUSTOMER NAME, JOB NAME AND WO NUM TO "x"
IF RESET_PARAMS == 1
	EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "COMPONENTS"
	
	COMP_ROW = 6
	EXCEL_GET_VALUE COMP_ROW 0 ITEM_NUM
	EXCEL_GET_VALUE COMP_ROW 1 SUB_TYPE
	EXCEL_GET_VALUE COMP_ROW 2 QTY
	
	IF ITEM_NUM <> ""
		EOF = "FALSE"
	ELSE
		EOF = "TRUE"
	END_IF

	COMP_ROW = 6

	WHILE EOF == "FALSE"
		IF QTY > 0
			IF SUB_TYPE == "ASSEMBLY" OR SUB_TYPE == "ASM"
				RETRIEVE_MDL ITEM_NUM+".ASM" ASSEM
				CLEAR_ARRAY ALL_COMPONENTS
				SEARCH_MDL_REFS ASSEM COMPONENT "*" ALL_COMPONENTS
			ELSE_IF SUB_TYPE == "PART"
				RETRIEVE_MDL ITEM_NUM+".PRT" ASSEM
				CLEAR_ARRAY ALL_COMPONENTS
				ADD_ARRAY_ELEM ALL_COMPONENTS ASSEM
			END_IF

			SET_MDL_PARAM ASSEM "USED" 0
			SET_MDL_PARAM ASSEM "CUSTOMER_NAME" "x"
			SET_MDL_PARAM ASSEM "JOB_NAME" "x"
			SET_MDL_PARAM ASSEM "SUB_JOB_NAME" "x"
			SET_MDL_PARAM ASSEM "WO_NUM" "x"
			
			SEARCH_MDL_PARAM ASSEM "EMJACNUM" EMJACNUM
			TEMPNUM = strfind (EMJACNUM, WO_NUM)
			IF TEMPNUM >= 0
				SET_MDL_PARAM ASSEM "EMJACNUM" ""
			END_IF			

			! REGEN_MDL ASSEM
			SAVE_MDL ASSEM
		END_IF
		
		COMP_ROW++
		EXCEL_GET_VALUE COMP_ROW 0 ITEM_NUM
		
		IF ITEM_NUM <> ""
			EOF = "FALSE"
			EXCEL_GET_VALUE COMP_ROW 1 SUB_TYPE
			EXCEL_GET_VALUE COMP_ROW 2 QTY
		ELSE
			EOF = "TRUE"
		END_IF
	END_WHILE			
END_IF

EXCEL_SAVE_DOCUMENT BOM_EXCEL
EXCEL_CLOSE_DOCUMENT

EXCEL_DISCONNECT
ERASE_NOT_DISPLAYED
WINDOW_CLOSE

