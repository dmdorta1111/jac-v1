				END_IF

				GET_ARRAY_ELEM ITEMS (((ITEM_INDEX-1)*20)+14) COMMENTS
				IF COMMENTS <> ""
					SET_DRW_TABLE_TEXT refDrawing BODY LINE_INDEX 8 COMMENTS
				END_IF				

				LINE_INDEX++
				ITEM_INDEX++
				IF ITEM_INDEX <= LISTING_ROW-(LISTING_ROW-ITP)
					GET_ARRAY_ELEM ITEMS (((ITEM_INDEX-1)*20)+17) SHAPE2
