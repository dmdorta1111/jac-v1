	READ_SUBDIRECTORIES . WORK_ORDERS
	EXISTING_DIRECTORY = -1

	FOR WORK_ORDER REF ARRAY WORK_ORDERS
		WORK_ORDER = WORK_ORDER + "ZYXWVU"
		EXISTING_DIRECTORY = strfind (WO_NUM, WORK_ORDER)
		IF EXISTING_DIRECTORY <> -1
			SET_WORKING_DIRECTORY WO_DIRECTORY
			BREAK
		END_IF
	END_FOR

	IF EXISTING_DIRECTORY == -1
		CREATE_DIRECTORY WO_DIRECTORY
		SET_WORKING_DIRECTORY WO_DIRECTORY
	END_IF
	
	DXF_DIRECTORY = WO_DIRECTORY+"\\"+WO_NUM
	READ_SUBDIRECTORIES . DIRECTORIES
	EXISTING_DIRECTORY = -1

	FOR DIRECTORY REF ARRAY DIRECTORIES
		EXISTING_DIRECTORY = strfind (WO_NUM, WORK_ORDER)
		IF EXISTING_DIRECTORY <> -1
			SET_WORKING_DIRECTORY DXF_DIRECTORY
			BREAK
		END_IF
	END_FOR

	IF EXISTING_DIRECTORY == -1
		CREATE_DIRECTORY DXF_DIRECTORY
		SET_WORKING_DIRECTORY DXF_DIRECTORY
	END_IF
	
	IF WO_CHOICE == 3
		BOM_DOCUMENT_NAME = "BOM_"+WO_NUM
		EXISTING_EXCEL = -1
		READ_DIRECTORY DXF_DIRECTORY "*.xls" EXCEL_FILES
		GET_ARRAY_SIZE EXCEL_FILES sizeTestArray
		
		IF sizeTestArray > 0
			FOR BOM_DOCUMENT REF ARRAY EXCEL_FILES
				EXISTING_EXCEL = strfind (BOM_DOCUMENT, BOM_DOCUMENT_NAME)
				IF EXISTING_EXCEL <> -1
					EXCEL_CONNECT !INVISIBLE
					EXCEL_LOAD_DOCUMENT BOM_DOCUMENT_NAME
					EXCEL_ACTIVATE_DOCUMENT BOM_DOCUMENT_NAME
					EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "LISTING"
					BREAK
				END_IF
			END_FOR
		END_IF

		IF EXISTING_EXCEL == -1
			EXCEL_CONNECT !INVISIBLE
			EXCEL_LOAD_DOCUMENT "C:\SIGMAXIM\Library\component_engine\Process\BOM_TEMPLATE.xls"
			EXCEL_ACTIVATE_DOCUMENT "C:\SIGMAXIM\Library\component_engine\Process\BOM_TEMPLATE.xls"
			EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "LISTING"
			EXCEL_SET_VALUE CELL_BY_NAME "B2" WO_NUM
			SET_WORKING_DIRECTORY DXF_DIRECTORY
			EXCEL_SAVE_DOCUMENT BOM_DOCUMENT_NAME
			SET_WORKING_DIRECTORY WO_DIRECTORY
		END_IF
	END_IF

	FOR ITP REF ARRAY ITEMS_TO_PROCESS
		ERASE_NOT_DISPLAYED	
		EXCEL_ACTIVATE_DOCUMENT SO_NUM
		EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "OPENING"
		ITEM_NUM = ""
		EXCEL_COLUMN = 1
		WHILE ITEM_NUM <> ITP
			EXCEL_GET_VALUE 5 EXCEL_COLUMN ITEM_NUM
			IF ITEM_NUM <> ITP
				IF ITEM_NUM <> ""
					EXCEL_COLUMN ++
				ELSE
					EXIT
				END_IF
			END_IF
		END_WHILE

		EXCEL_GET_VALUE 296 EXCEL_COLUMN SHEET_QTY
		
		IF WO_TYPE == "F"
			SET_WORKING_DIRECTORY WO_DIRECTORY
			RETRIEVE_MDL WO_NUM+"_F-"+ ITEM_NUM+".ASM" ASSEM
		END_IF
		IF WO_TYPE == "D"
			SET_WORKING_DIRECTORY WO_DIRECTORY
			RETRIEVE_MDL WO_NUM+"_D-"+ ITEM_NUM+".ASM" ASSEM
		END_IF

		REGEN_MDL ASSEM FORCE
		REGEN_MDL ASSEM FORCE
		CALL PARTS_LIST.TAB
	END_FOR
	
	SET_WORKING_DIRECTORY WO_DIRECTORY
	
	CALL EXPORT_SA.TAB
	
	DECLARE_VARIABLE STRING ITEM_TO_PDF

	FOR ITP REF ARRAY ITEMS_TO_PROCESS
	
		SET_WORKING_DIRECTORY WO_DIRECTORY
		
		IF WO_TYPE == "F"
			ITEM_TO_PDF = WO_NUM+"_F-"+ ITP+".drw"
		END_IF

		IF WO_TYPE == "D"
			ITEM_TO_PDF = WO_NUM+"_D-"+ ITP+".drw"
		END_IF
		
		RETRIEVE_MDL &ITEM_TO_PDF refDrawing
		SET_LAYER_STATUS RECURSIVE ASSEM "TEMPLATES" BLANK
		SET_LAYER_STATUS RECURSIVE ASSEM "COMPONENTS" DISPLAY
		
		REGEN_MDL RefDrawing

		SET_WORKING_DIRECTORY DXF_DIRECTORY
		SET_CONFIG_OPTION "system_colors_file" "C:\SIGMAXIM\Library\component_engine\SDI_New\sa.scl"		
		DECLARE_STRUCT PDF_OPTION option
		option.title = "sample drawing"
		option.author = "DRAFTER"
		option.subject = "detail"
		option.keywords = "SmartAssembly example"
		option.launch_viewer = "FALSE"
		option.color_depth = "gray"
		option.layer_mode = "None"
		option.use_pentable = "TRUE"
		option.pentable_file = "C:\pro_stds\ProStandards\emjac_table.pnt"
		! option.use_true_type_fonts = 1
		SET_CONFIG_OPTION "system_colors_file" "C:\SIGMAXIM\Library\component_engine\SDI_New\initial.scl"
		
		IF WO_TYPE == "F"
			EXPORT_DRW_PDF RefDrawing option WO_NUM+"_F-"+ ITP
			DECLARE_VARIABLE STRING TEMP_NAME
			TEMP_NAME = WO_NUM+"_F-"+ ITP
			PRINT "%" TEMP_NAME
		END_IF
		
		IF WO_TYPE == "D"
			! IF SUB_TYPE == "P"
				DECLARE_VARIABLE DOUBLE TABLELOCY
				SEARCH_MDL_REFS RefDrawing DRAWING_TABLE "*" RefTables
				FOR RefTable REF ARRAY RefTables
					GET_REF_ID RefTable tab_id
					GET_DRW_TABLE_OUTLINE RefDrawing tab_id x1 y1 x2 y2
PRINT "%" tab_id
PRINT "%" x1
PRINT "%" y1
PRINT "%" x2
PRINT "%" y2
						IF x1 == 11.0625 AND y1 == 10.375
							TABLELOCY = y2
						END_IF
						IF x1 == 11.0625 AND y1 <> 10.375
							SET_DRW_TABLE_POS RefDrawing tab_id x1 TABLELOCY-.0625
							UPDATE_DRW_VIEWS RefDrawing 1
							BREAK
						END_IF
				END_FOR
			! END_IF
			EXPORT_DRW_PDF RefDrawing option WO_NUM+"_D-"+ ITP
			DECLARE_VARIABLE STRING TEMP_NAME
			TEMP_NAME = WO_NUM+"_F-"+ ITP
			PRINT "%" TEMP_NAME
			
			IF DOOR_CONSTRUCTION == "LOCKSEAM" AND HINGE_EDGE_SHAPE == "BEVEL" AND STRIKE_EDGE_SHAPE == "BEVEL"

				RETRIEVE_MDL LOCKSEAM_FORMING_DIMS.DRW PDF_DRW	

				DECLARE_STRUCT PDF_OPTION option
				option.title = "sample drawing"
				option.author = "DRAFTER"
				option.subject = "detail"
				option.keywords = "SmartAssembly example"
				option.launch_viewer = "FALSE"
				option.color_depth = "gray"
				option.layer_mode = "None"
				option.use_pentable = "TRUE"
				option.pentable_file = "C:\pro_stds\ProStandards\emjac_table.pnt"
				! option.use_true_type_fonts = 1
				EXPORT_DRW_PDF PDF_DRW option "LOCKSEAM_FORMING_DIMS"
			END_IF
		END_IF
	END_FOR
END_IF

SET_WORKING_DIRECTORY PROJECT_DIRECTORY
EXCEL_ACTIVATE_DOCUMENT SO_NUM
EXCEL_SAVE_DOCUMENT SO_NUM
EXCEL_DISCONNECT
! ERASE_NOT_DISPLAYED

END_ASM_DESCR


