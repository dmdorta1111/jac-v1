BEGIN_GUI_DESCR

	! GLOBAL_PICTURE BLANK.GIF

	SHOW_PARAM STRING CUSTOMER_NAME
	SHOW_PARAM STRING JOB_NAME
	SHOW_PARAM STRING SO_NUM
	SHOW_PARAM STRING DOOR_ITEM_DESC
	SHOW_PARAM STRING FRAME_ITEM_DESC
	SHOW_PARAM STRING SELECTED_HAND
	SHOW_PARAM STRING STD_REV

	USER_INPUT_PARAM 	STRING 	CUSTOMER_NAME
	USER_INPUT_PARAM 	STRING 	JOB_NAME
	USER_INPUT_PARAM 	STRING 	SUB_JOB_NAME
	USER_INPUT_PARAM 	STRING 	ITEM_NUM REQUIRED
	USER_INPUT_PARAM 	INTEGER QTY REQUIRED
	USER_INPUT_PARAM	DOUBLE	JOW REQUIRED
	USER_INPUT_PARAM	DOUBLE	JOH	REQUIRED
	
	USER_INPUT_PARAM	DOUBLE JD 	REQUIRED
	
	USER_INPUT_PARAM	INTEGER	FIRE_RATING REQUIRED
	USER_INPUT_PARAM	STRING 	COMMENTS
	! USER_INPUT_PARAM	STRING 	COMMENTS_2
	! USER_INPUT_PARAM	STRING 	COMMENTS_3
	! USER_INPUT_PARAM	STRING 	COMMENTS_4
	! USER_INPUT_PARAM	STRING 	COMMENTS_5

	CHECKBOX_PARAM INTEGER SUBMITTAL "Yes/No"
	CHECKBOX_PARAM INTEGER APPROVAL "Yes/No"
	CHECKBOX_PARAM INTEGER DOOR_LABEL_CONSTRUCTION "Yes/No"
	CHECKBOX_PARAM INTEGER MULTIPLE_TAGS "Yes/No"
	
	RADIOBUTTON_PARAM INTEGER OPENING_TYPE "NON SELECTED" "DOOR ONLY" "FRAME ONLY" "BOTH" "BORROWED LITE"
		IF OPENING_TYPE == 0
			DOOR_ITEM_DESC = "NONE"
			FRAME_ITEM_DESC = "NONE"
		END_IF
		IF OPENING_TYPE == 1 OR OPENING_TYPE == 3
			DOOR_ITEM_DESC = "DOOR"
		ELSE
			DOOR_ITEM_DESC = "NONE"
		END_IF
		IF OPENING_TYPE == 2 OR OPENING_TYPE == 3 OR OPENING_TYPE == 4
			FRAME_ITEM_DESC = "FRAME"
		ELSE
			FRAME_ITEM_DESC = "NONE"
			FRAME_ELEVATION = 0
		END_IF

	IF OPENING_TYPE <> 4
		RADIOBUTTON_PARAM INTEGER HANDI_ " LH" "RH" "LHA" "RHA" "LH/RH" " LHR" "RHR" "LHRA" "RHRA" "LHR/RHR" "NO HANDING" "SELECT HAND"
			IF HANDI_ == 0
				HAND = "LH"
				SELECTED_HAND = "LH"
				SUB_TYPE = "S"
			ELSE_IF HANDI_ == 1
				HAND = "RH"
				SELECTED_HAND = "RH"
				SUB_TYPE = "S"
			ELSE_IF HANDI_ == 2
				HAND = "LHA"
				SELECTED_HAND = "LHA"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 3
				HAND = "RHA"
				SELECTED_HAND = "RHA"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 4
				HAND = "LH/RH"
				SELECTED_HAND = "LH/RH"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 5
				HAND = "LHR"
				SELECTED_HAND = "LHR"
				SUB_TYPE = "S"
			ELSE_IF HANDI_ == 6
				HAND = "RHR"
				SELECTED_HAND = "RHR"
				SUB_TYPE = "S"
			ELSE_IF HANDI_ == 7
				HAND = "LHRA"
				SELECTED_HAND = "LHRA"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 8
				HAND = "RHRA"
				SELECTED_HAND = "RHRA"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 9
				HAND = "LHR/RHR"
				SELECTED_HAND = "LHR/RHR"
				SUB_TYPE = "P"
			ELSE_IF HANDI_ == 10
				HAND = "NONE"
				SELECTED_HAND = "NONE"
				USER_INPUT_PARAM INTEGER DOOR_QTY REQUIRED
				IF DOOR_QTY == 1 OR DOOR_QTY == 0
					SUB_TYPE = "S"
				ELSE_IF DOOR_QTY == 2
					SUB_TYPE = "P"
				ELSE
					SUB_TYPE = "N"
				END_IF
			ELSE_IF HANDI_ == 11
				SELECTED_HAND = "SELECT"
			END_IF

		IF HAND == "LH" OR HAND == "LHR" OR HAND == "RH" OR HAND == "RHR"
			SUB_TYPE = "S"
		ELSE_IF HAND == "LH/RH" OR HAND == "LHA" OR HAND == "RHA" OR \
		  HAND == "LHR/RHR" OR HAND == "LHRA" OR HAND == "RHRA"
			SUB_TYPE = "P"
		END_IF

		IF HAND == "LH" OR HAND == "RH" OR HAND == "LH/RH" OR HAND == "LHA" OR HAND == "RHA"
			STD_REV = "STD"
		ELSE_IF HAND == "LHR" OR HAND == "RHR" OR HAND == "LHR/RHR" OR HAND == "LHRA" OR HAND == "RHRA"
			STD_REV = "REV"
		ELSE
			STD_REV = "STD"		
		END_IF		
	END_IF

	IF OPENING_TYPE == 4
		HAND = "BL"
	END_IF
	! CHECKBOX_PARAM INTEGER SDI_LOCATION "YES/NO"

	! IF SDI_LOCATION == 1
		! MANUFACTURERS_LOCATIONS = "SDI"
	! ELSE
		! MANUFACTURERS_LOCATIONS = "X"
	! END_IF

END_GUI_DESCR

