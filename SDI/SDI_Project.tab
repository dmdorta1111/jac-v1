	ADD_ARRAY_ELEM CHOICES "PROJECT STANDARDS"		!10
	

	MESSAGE_BOX_EX QUESTION "OPERATION" "PLEASE SELECT" CHOICES CHOICE

	IF CHOICE == 0

		EXCEL_DISCONNECT
