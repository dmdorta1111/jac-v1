GET_WORKING_DIRECTORY CURRENT_DIRECTORY
PRINT "IS THIS THE CORRECT DIRECTORY?  %" CURRENT_DIRECTORY
PRINT "HIT ANY CHARACTER TO CHANGE DIRECTORIES, RETURN TO ACCEPT CURRENT"
USER_INPUT_PARAM STRING CORRECT_DIRECTORY
IF CORRECT_DIRECTORY <> ""
	! SO_NUM = "71621"
	USER_INPUT_PARAM STRING SO_NUM
	SET_WORKING_DIRECTORY "C:\000_SDI"
	PROJECT_DIRECTORY = "C:\000_SDI\\" + SO_NUM
	READ_SUBDIRECTORIES . PROJECTS
	EXISTING_DIRECTORY = -1

	FOR PROJECT REF ARRAY PROJECTS
		EXISTING_DIRECTORY = strfind (PROJECT, SO_NUM)
		IF EXISTING_DIRECTORY <> -1
			PRINT "%" PROJECT_DIRECTORY
			SET_WORKING_DIRECTORY PROJECT_DIRECTORY
			BREAK
		END_IF
	END_FOR

	IF EXISTING_DIRECTORY == -1
		CREATE_DIRECTORY PROJECT_DIRECTORY
		SET_WORKING_DIRECTORY PROJECT_DIRECTORY
		CREATE_DIRECTORY "Customer Drawings"
		CREATE_DIRECTORY "ProE Models"
	END_IF
ELSE
	DECLARE_VARIABLE INTEGER STRLENGTH
	STRLENGTH = strlen(CURRENT_DIRECTORY)
	PRINT "%" STRLENGTH
	SO_NUM = strmid(CURRENT_DIRECTORY,STRLENGTH-6, 5)
	PRINT "%" SO_NUM
	PROJECT_DIRECTORY = "C:\000_SDI\\" + SO_NUM
END_IF

CLEAR_ARRAY EXCEL_FILES
EXCEL_DOCUMENT_NAME = SO_NUM
EXISTING_EXCEL = -1
READ_DIRECTORY PROJECT_DIRECTORY "*.xls" EXCEL_FILES
GET_ARRAY_SIZE EXCEL_FILES sizeTestArray

IF sizeTestArray > 0
	FOR EXCEL_DOCUMENT REF ARRAY EXCEL_FILES
		EXISTING_EXCEL = strfind (EXCEL_DOCUMENT, EXCEL_DOCUMENT_NAME)
		IF EXISTING_EXCEL <> -1
			EXCEL_CONNECT !INVISIBLE
			EXCEL_LOAD_DOCUMENT EXCEL_DOCUMENT
			EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "OPENING"
			EXCEL_GET_VALUE CELL_BY_NAME "B1" CUSTOMER_NAME
			EXCEL_GET_VALUE CELL_BY_NAME "B2" JOB_NAME
			! EXCEL_GET_VALUE CELL_BY_NAME "B3" SO_NUM
			BREAK
		END_IF
	END_FOR
END_IF

IF EXISTING_EXCEL == -1
	EXCEL_CONNECT !INVISIBLE
	EXCEL_LOAD_DOCUMENT "C:\SIGMAXIM\Library\component_engine\SDI_New\SO_TEMPLATE.xls"
	EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "OPENING"
	USER_INPUT_PARAM STRING CUSTOMER_NAME
	EXCEL_SET_VALUE CELL_BY_NAME "B1" CUSTOMER_NAME
	USER_INPUT_PARAM STRING JOB_NAME
	EXCEL_SET_VALUE CELL_BY_NAME "B2" JOB_NAME
	EXCEL_SET_VALUE CELL_BY_NAME "B3" SO_NUM
	EXCEL_SAVE_DOCUMENT EXCEL_DOCUMENT_NAME
END_IF


CHOICE = 3
WHILE CHOICE == 3 OR CHOICE == 4
