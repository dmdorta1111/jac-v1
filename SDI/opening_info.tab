			IF PRIMARY_LOCK_TYPE <> "" AND PRIMARY_LOCK_TYPE <> "NONE"
				DECLARE_VARIABLE STRING LOCK_TAB ""
				DECLARE_VARIABLE INTEGER TEMP_1	0
				DECLARE_VARIABLE INTEGER TEMP_2 0

				TEMP_2 = strfind (PRIMARY_LOCK_TYPE, " ")
				IF TEMP_2 > 0
					TEMP_1 = strlen (PRIMARY_LOCK_TYPE)
					LOCK_TAB = strleft (PRIMARY_LOCK_TYPE,TEMP_2)
					LOCK_TAB = LOCK_TAB + "_" + strright (PRIMARY_LOCK_TYPE,TEMP_1 - TEMP_2-1)
					TEMP_2 = strfind (LOCK_TAB, " ")
					WHILE TEMP_2 > 0
						TEMP_1 = strlen (LOCK_TAB)
						LOCK_TAB = LOCK_TAB + "_" + strright (LOCK_TAB,TEMP_1 - TEMP_2-1)
						TEMP_2 = strfind (LOCK_TAB, " ")
					END_WHILE
					LOCK_TAB = LOCK_TAB + ".TAB"
				ELSE
					LOCK_TAB = PRIMARY_LOCK_TYPE + ".TAB"
				END_IF

				LOCK_INDEX = 1
				CALL LOCK_TAB
			END_IF

