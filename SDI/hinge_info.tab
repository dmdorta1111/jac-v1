BEGIN_ASM_DESCR

DECLARE_VARIABLE	INTEGER 	CALCULATED_HINGE_QTY
DECLARE_VARIABLE	INTEGER 	CUSTOM_HINGE_QTY
DECLARE_VARIABLE	STRING 		DIMENSION_METHOD_TC
DECLARE_VARIABLE	DOUBLE 	DOOR_FACE_SETBACK
DECLARE_VARIABLE	DOUBLE 	DOOR_GAP
DECLARE_VARIABLE	DOUBLE 	BUTT_HINGE_HEIGHT_ALLOWANCE
DECLARE_VARIABLE	DOUBLE 	DOOR_HINGE_SETBACK
DECLARE_VARIABLE	DOUBLE		BUTT_HINGE_THICKNESS_ALLOWANCE
DECLARE_VARIABLE	DOUBLE		BUTT_HINGE_WIDTH_ALLOWANCE
DECLARE_VARIABLE	INTEGER 	ELEC_HINGE_DOOR
DECLARE_VARIABLE	STRING		ELEC_HINGE_MODEL
DECLARE_VARIABLE	STRING		ELEC_HINGE_MODEL_RIGHT
DECLARE_VARIABLE	STRING 		HINGE_DEC
DECLARE_VARIABLE	INTEGER 	HINGE_INDEX 0
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_1
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_2
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_3
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_4
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_5
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_6
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_7
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_8
DECLARE_VARIABLE	DOUBLE		HINGE_LOCATION_9
DECLARE_VARIABLE	STRING 		HINGE_MANUFACTURER
DECLARE_VARIABLE	STRING 		HINGE_MODEL
DECLARE_VARIABLE	INTEGER 	HINGE_QTY
DECLARE_VARIABLE	INTEGER 	EQUAL_SPACING
DECLARE_VARIABLE	DOUBLE 	HINGE_SPACING
DECLARE_VARIABLE	INTEGER 	ID_ROW
DECLARE_VARIABLE	INTEGER 	INSTAL_HINGE
DECLARE_VARIABLE	DOUBLE 	JAMB_HINGE_SETBACK
DECLARE_VARIABLE	DOUBLE 	JOH
DECLARE_VARIABLE	STRING 		LOCS
DECLARE_VARIABLE	STRING 		MANUFACTURERS_LOCATIONS
DECLARE_VARIABLE	DOUBLE 	MAXIMUM_HINGE_SPACING
DECLARE_VARIABLE	DOUBLE 	PRIMARY_STRIKE_CENTER
DECLARE_VARIABLE	BOOL  		STDS_EOF FALSE
DECLARE_VARIABLE	STRING 		STDS_LOCATIONS
DECLARE_VARIABLE	INTEGER		STDS_ROW 0
DECLARE_VARIABLE	INTEGER 	TEST_STDS_EOF 0
DECLARE_VARIABLE	DOUBLE		TOP_GAP
DECLARE_VARIABLE	BOOL  		VALID_HDWE_ID FALSE
DECLARE_VARIABLE	INTEGER 	WELDED_BOXES
DECLARE_VARIABLE	DOUBLE 	TEMPLATE_HINGE_GAP
DECLARE_VARIABLE	DOUBLE 	HINGE_GAP_ADD
DECLARE_VARIABLE	STRING		HINGE_EDGE_SHAPE
DECLARE_VARIABLE	INTEGER		ALT_STRIKE_CENTER_
DECLARE_VARIABLE	DOUBLE		ALT_STRIKE_CENTER
DECLARE_VARIABLE	STRING		SUB_TYPE
DECLARE_VARIABLE	STRING		HINGE_UDF
DECLARE_VARIABLE	INTEGER		THRU_BOLT
DECLARE_VARIABLE	INTEGER		CONTINUOUS_HINGE_REINFORCEMENT
DECLARE_VARIABLE	INTEGER		HINGE_DIMENSION_METHOD

DECLARE_ARRAY HINGE_LOCATIONS_ARRAY
ID_ROW = size
! HINGE_QTY_ = HINGE_QTY

IF MANUFACTURERS_LOCATIONS == "CUSTOM"
	CUSTOM_HINGE_QTY = HINGE_QTY
END_IF

! CONFIG_ELEM

IF MANUFACTURERS_LOCATIONS == "N/A"
	HINGE_TYPE = "NONE"
END_IF

IF HINGE_TYPE <> "NONE"
	IF ELECTRIC_HINGE_POSITION == 4
		ELECTRIC_HINGE_POSITION = 97
	ELSE_IF ELECTRIC_HINGE_POSITION == 5
		ELECTRIC_HINGE_POSITION = 98
	ELSE_IF ELECTRIC_HINGE_POSITION == 6
		ELECTRIC_HINGE_POSITION = 99
	END_IF

	IF DOOR_CONSTRUCTION == "SEAMLESS" AND WELDED_BOXES == 1
		BUTT_HINGE_HEIGHT_ALLOWANCE = .18
		BUTT_HINGE_WIDTH_ALLOWANCE = 0
		BUTT_HINGE_THICKNESS_ALLOWANCE = .015
	ELSE_IF DOOR_CONSTRUCTION == "SEAMLESS" AND WELDED_BOXES == 0
		BUTT_HINGE_HEIGHT_ALLOWANCE = .005
		BUTT_HINGE_WIDTH_ALLOWANCE = 0
		BUTT_HINGE_THICKNESS_ALLOWANCE = .015
	ELSE_IF DOOR_CONSTRUCTION == "LOCKSEAM" AND WELDED_BOXES == 0
		BUTT_HINGE_HEIGHT_ALLOWANCE = .005
		BUTT_HINGE_WIDTH_ALLOWANCE = 0.005
		BUTT_HINGE_THICKNESS_ALLOWANCE = .030
	END_IF

	IF (HINGE_TYPE == "BUTT" OR HINGE_TYPE == HALF_MORTISE) AND MANUFACTURERS_LOCATIONS == "SDI"
		! IF HINGE_HEIGHT == 4.5
			MAXIMUM_HINGE_SPACING = 35.2
			TOP_HINGE = 7.25
			BOTTOM_HINGE = 12.25
		! END_IF
		! IF HINGE_HEIGHT == 5.0
			! MAXIMUM_HINGE_SPACING = 24
			! TOP_HINGE = 7.5
			! BOTTOM_HINGE = 12.5
		! END_IF
		
		HINGE_QTY = ceil((JOH-TOP_HINGE-BOTTOM_HINGE)/MAXIMUM_HINGE_SPACING) +1
		HINGE_SPACING = (JOH-TOP_HINGE-BOTTOM_HINGE) / (HINGE_QTY - 1)
		JAMB_HINGE_SETBACK = .375
		DOOR_HINGE_SETBACK = .28125
		DOOR_FACE_SETBACK = .09375
		DOOR_GAP = .09375

		CLEAR_ARRAY HINGE_LOCATIONS_ARRAY
		ADD_ARRAY_ELEM HINGE_LOCATIONS_ARRAY TOP_HINGE
		HINGE_INDEX = 1
		WHILE HINGE_INDEX <= HINGE_QTY-1
			ADD_ARRAY_ELEM HINGE_LOCATIONS_ARRAY TOP_HINGE+(HINGE_INDEX * HINGE_SPACING)
			HINGE_INDEX++
		END_WHILE
	ELSE_IF (HINGE_TYPE == "BUTT" OR HINGE_TYPE == HALF_MORTISE) AND MANUFACTURERS_LOCATIONS <> "SDI" AND MANUFACTURERS_LOCATIONS <> "CUSTOM" AND MANUFACTURERS_LOCATIONS <> "PROJECT"
		EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "LOCATIONS"
		VALID_HDWE_ID = FALSE
		STDS_EOF = FALSE
		STDS_ROW = 0
		WHILE VALID_HDWE_ID == FALSE AND STDS_EOF == FALSE
			EXCEL_GET_VALUE STDS_ROW 0 LOCS
			EXCEL_GET_VALUE STDS_ROW 1 HGE_HGT
			EXCEL_GET_VALUE STDS_ROW 2 STD_JOH
					IF LOCS == MANUFACTURERS_LOCATIONS AND HGE_HGT == HINGE_HEIGHT AND STD_JOH == JOH
						IF ALT_STRIKE_CENTER_ == 0
							EXCEL_GET_VALUE STDS_ROW 3 PRIMARY_STRIKE_CENTER
						ELSE
							PRIMARY_STRIKE_CENTER = ALT_STRIKE_CENTER
						END_IF

						EXCEL_GET_VALUE STDS_ROW 4 HINGE_QTY
						EXCEL_GET_VALUE STDS_ROW 5 BOTTOM_HINGE
						EXCEL_GET_VALUE STDS_ROW 6 HINGE_SPACING
						EXCEL_GET_VALUE STDS_ROW 7 TOP_HINGE
						EXCEL_GET_VALUE STDS_ROW 8 JAMB_HINGE_SETBACK
						EXCEL_GET_VALUE STDS_ROW 9 DOOR_HINGE_SETBACK
						EXCEL_GET_VALUE STDS_ROW 10 DOOR_FACE_SETBACK
						EXCEL_GET_VALUE STDS_ROW 11 DOOR_GAP
						VALID_HDWE_ID = TRUE
							ADD_ARRAY_ELEM HINGE_LOCATIONS_ARRAY TOP_HINGE
							HINGE_INDEX = 1
							WHILE HINGE_INDEX <= HINGE_QTY-1
								ADD_ARRAY_ELEM HINGE_LOCATIONS_ARRAY TOP_HINGE+(HINGE_INDEX * HINGE_SPACING)
								HINGE_INDEX++
							END_WHILE
					END_IF
			STDS_ROW ++
			EXCEL_GET_VALUE STDS_ROW 0 TEST_STDS_EOF
			IF TEST_STDS_EOF == ""
				STDS_EOF = TRUE
			END_IF
		END_WHILE
		IF VALID_HDWE_ID == FALSE AND STDS_EOF == TRUE
			STDS_LOCATIONS = "CUSTOM"
		END_IF
	END_IF

	IF CUSTOM_HINGE_QTY > 0
		IF CUSTOM_HINGE_QTY == CALCULATED_HINGE_QTY
			HINGE_QTY = CALCULATED_HINGE_QTY
			CUSTOM_HINGE_QTY = 0
		ELSE
			HINGE_QTY = CUSTOM_HINGE_QTY	
		END_IF
	END_IF

	IF (HINGE_TYPE == "BUTT" OR HINGE_TYPE == HALF_MORTISE OR HINGE_TYPE == "DBL_ACTING" OR HINGE_TYPE == "CONCEALED") AND (MANUFACTURERS_LOCATIONS == "CUSTOM" OR STDS_LOCATIONS == "CUSTOM" OR CUSTOM_HINGE_QTY > 0)
		CLEAR_ARRAY HINGE_LOCATIONS_ARRAY
		CALL_EX custom_hinges.tab
			VAR_INOUT HINGE_QTY							HINGE_QTY
			VAR_INOUT CUSTOM_HINGE_QTY					CUSTOM_HINGE_QTY
			VAR_INOUT HINGE_HEIGHT						HINGE_HEIGHT
			VAR_INOUT TOP_HINGE							TOP_HINGE
			VAR_INOUT BOTTOM_HINGE						BOTTOM_HINGE
			VAR_INOUT HINGE_LOCATIONS_ARRAY				HINGE_LOCATIONS_ARRAY
			VAR_INOUT TOP_GAP							TOP_GAP
			VAR_INOUT DOOR_GAP							DOOR_GAP
			VAR_INOUT JOH								JOH
			VAR_INOUT JAMB_HINGE_SETBACK					JAMB_HINGE_SETBACK
			VAR_INOUT DOOR_HINGE_SETBACK					DOOR_HINGE_SETBACK
			VAR_INOUT DOOR_FACE_SETBACK					DOOR_FACE_SETBACK
			VAR_INOUT MAXIMUM_HINGE_SPACING				MAXIMUM_HINGE_SPACING
			VAR_INOUT HINGE_SPACING						HINGE_SPACING
			VAR_INOUT HINGE_LOCATION_1					HINGE_LOCATION_1
			VAR_INOUT HINGE_LOCATION_2					HINGE_LOCATION_2
			VAR_INOUT HINGE_LOCATION_3					HINGE_LOCATION_3
			VAR_INOUT HINGE_LOCATION_4					HINGE_LOCATION_4
			VAR_INOUT HINGE_LOCATION_5					HINGE_LOCATION_5
			VAR_INOUT HINGE_LOCATION_6					HINGE_LOCATION_6
			VAR_INOUT HINGE_LOCATION_7					HINGE_LOCATION_7
			VAR_INOUT HINGE_LOCATION_8					HINGE_LOCATION_8
			VAR_INOUT HINGE_LOCATION_9					HINGE_LOCATION_9
			VAR_INOUT EQUAL_SPACING						EQUAL_SPACING
			VAR_INOUT HINGE_DIMENSION_METHOD				HINGE_DIMENSION_METHOD			
		END_CALL_EX
	END_IF

	IF ELECTRIC_HINGE_POSITION == 97
		ELECTRIC_HINGE_POSITION = HINGE_QTY - 1
	END_IF

	IF HINGE_TYPE <> "CONTINUOUS" AND HINGE_TYPE <> "ELIASON"
		DECLARE_VARIABLE INTEGER HINGE_ROW 20
		DECLARE_VARIABLE INTEGER ARRAY_INDEX 0
		DECLARE_VARIABLE INTEGER ARRAY_SIZE 0

		DECLARE_VARIABLE DOUBLE VALUE
		GET_ARRAY_SIZE HINGE_LOCATIONS_ARRAY ARRAY_SIZE

		IF ARRAY_INDEX <= ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_1 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_1 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_2 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_2 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_3 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_3 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_4 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_4 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_5 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_5 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_6 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_6 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_7 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_7 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_8 = VALUE
			ARRAY_INDEX++
		ELSE
			HINGE_LOCATION_8 = 0
		END_IF
		IF ARRAY_INDEX < ARRAY_SIZE
			GET_ARRAY_ELEM HINGE_LOCATIONS_ARRAY ARRAY_INDEX VALUE
			HINGE_LOCATION_9 = VALUE
		ELSE
			HINGE_LOCATION_9 = 0
		END_IF
	END_IF

	IF HINGE_TYPE == "CONTINUOUS"
		HINGE_QTY = 1
		HINGE_HEIGHT = 0					
		HINGE_THICKNESS = 0			
		HINGE_SPACING = 0					
		TOP_HINGE = 0						
		BOTTOM_HINGE = 0						
		ELECTRIC_HINGE_QTY = 0				
		BUTT_HINGE_HEIGHT_ALLOWANCE = 0		
		BUTT_HINGE_WIDTH_ALLOWANCE = 0		
		BUTT_HINGE_THICKNESS_ALLOWANCE = 0
	END_IF

	EXCEL_ACTIVATE_SHEET SHEET_BY_NAME "OPENING"			
	
END_IF
END_ASM_DESCR
