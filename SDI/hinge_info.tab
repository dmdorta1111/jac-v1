BEGIN_GUI_DESCR

	GLOBAL_PICTURE butt.gif
	
	SHOW_PARAM STRING HINGE_TYPE
	SHOW_PARAM STRING HINGE_GAP
	
	USER_INPUT_PARAM DOUBLE TOP_GAP

	USER_INPUT_PARAM STRING HINGE_MANUFACTURER
	
	IF HINGE_TYPE == "CONTINUOUS" ! OR MANUFACTURERS_LOCATIONS == "N/A" OR HINGE_MANUFACTURER == "GENERIC"
		USER_INPUT_PARAM STRING HINGE_MODEL
		USER_INPUT_PARAM DOUBLE TEMPLATE_HINGE_GAP
		USER_INPUT_PARAM DOUBLE HINGE_GAP_ADD
		HINGE_DESC = HINGE_MANUFACTURER + " - " + HINGE_MODEL + " - CONTINUOUS HINGE"
		IF HINGE_EDGE_SHAPE == "SQUARE"
			HINGE_GAP = TEMPLATE_HINGE_GAP
		END_IF
		IF HINGE_EDGE_SHAPE == "BEVEL"
			HINGE_GAP = TEMPLATE_HINGE_GAP + HINGE_GAP_ADD
		END_IF		
	ELSE
		TEMPLATE_HINGE_GAP = 0
		HINGE_GAP_ADD = 0
	END_IF

	CHECKBOX_PARAM INTEGER ALT_STRIKE_CENTER_ "(YES/NO)"
	
	IF ALT_STRIKE_CENTER_ == 1
		USER_INPUT_PARAM DOUBLE ALT_STRIKE_CENTER REQUIRED
	ELSE
		ALT_STRIKE_CENTER = 0
	END_IF
	
	IF HINGE_TYPE == "BUTT"
		USER_INPUT_PARAM INTEGER CUSTOM_HINGE_QTY
		RADIOBUTTON_PARAM INTEGER ELECTRIC_HINGE_POSITION "NONE" "TOP HINGE" "SECOND HINGE" "THIRD HINGE" "SECOND FROM BOTTOM" "BOTTOM HINGE" "ENTER"
		IF SUB_TYPE == "P"
			RADIOBUTTON_PARAM INTEGER ELEC_HINGE_DOOR "LEFT" "RIGHT" "BOTH"
		END_IF
	END_IF

	CHECKBOX_PARAM INTEGER WELDED_BOXES "(YES/NO)"

	IF HINGE_TYPE == "CONTINUOUS"
		CHECKBOX_PARAM INTEGER THRU_BOLT "(YES/NO)"
		CHECKBOX_PARAM INTEGER CONTINUOUS_HINGE_REINFORCEMENT "(YES/NO)"
	END_IF
	
	IF ELECTRIC_HINGE_POSITION == 6
		USER_INPUT_PARAM INTEGER ELECTRIC_HINGE_POSITION
	END_IF

	IF ELECTRIC_HINGE_POSITION > 0
		USER_INPUT_PARAM STRING ELEC_HINGE_MODEL
		IF SUB_TYPE == "P"
			USER_INPUT_PARAM STRING ELEC_HINGE_MODEL_RIGHT
		END_IF
	END_IF 
	
	IF HINGE_TYPE == "CONTINUOUS"
		CHECKBOX_PARAM INTEGER INSTAL_HINGE "(YES/NO)"
	END_IF
	
END_GUI_DESCR
