
BEGIN_GUI_DESCR

USER_INPUT_PARAM STRING TAG_NUM
USER_INPUT_PARAM INTEGER TAG_QTY

END_GUI_DESCR

BEGIN_ASM_DESCR

DECLARE_VARIABLE STRING TAG_NUM "X"
DECLARE_VARIABLE STRING SUB_TAGS ""
DECLARE_VARIABLE INTEGER SHEET_QTY

SHEET_QTY = QTY

WHILE TAG_NUM <> ""
	TAG_NUM = ""
	CONFIG_ELEM
	IF TAG_QTY > 0
		IF SUB_TAGS == ""
			SUB_TAGS = TAG_NUM +"^"+itos(TAG_QTY) 
		ELSE
			SUB_TAGS = SUB_TAGS+"^"+TAG_NUM +"^"+itos(TAG_QTY) 
		END_IF
		SHEET_QTY = SHEET_QTY + TAG_QTY
	END_IF
END_WHILE

EXCEL_SET_VALUE 294	EXCEL_COLUMN	SUB_TAGS
EXCEL_SET_VALUE 296	EXCEL_COLUMN	SHEET_QTY

END_ASM_DESCR

