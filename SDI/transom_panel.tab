BEGIN_GUI_DESCR
GLOBAL_PICTURE transom_panel.gif

USER_INPUT_PARAM DOUBLE TRANSOM_PANEL_HEIGHT REQUIRED
USER_INPUT_PARAM INTEGER TRANSOM_PANEL_GAUGE REQUIRED


RADIOBUTTON_PARAM INTEGER TRANSOM_PANEL_ATTACH_METHOD "LOOSE" "ANGLES" "WELDED"
RADIOBUTTON_PARAM INTEGER TRANSOM_PANEL_CORE "STYRENE" "URETHANE" "HONEYCOMB"


END_GUI_DESCR

BEGIN_ASM_DESCR

DECLARE_VARIABLE DOUBLE TRANSOM_PANEL_THICKNESS
DECLARE_VARIABLE STRING TRANSOM_PANEL_CORE_DESC



CONFIG_ELEM

IF TRANSOM_PANEL_CORE == 0
	TRANSOM_PANEL_CORE_DESC	 = "STYRENE"
ELSE_IF TRANSOM_PANEL_CORE == 1
	TRANSOM_PANEL_CORE_DESC	 = "URETHANE"
ELSE_IFTRANSOM_PANEL_CORE == 2
	TRANSOM_PANEL_CORE_DESC	 = "HONEYCOMB"
END_IF 

IF TRANSOM_PANEL_GAUGE == 18
	TRANSOM_PANEL_THICKNESS = .05
ELSE_IF TRANSOM_PANEL_GAUGE == 16
	TRANSOM_PANEL_THICKNESS = .06
ELSE_IF TRANSOM_PANEL_GAUGE == 14
	TRANSOM_PANEL_THICKNESS = .075
ELSE_IF TRANSOM_PANEL_GAUGE == 12
	TRANSOM_PANEL_THICKNESS = .105
END_IF

END_ASM_DESCR
