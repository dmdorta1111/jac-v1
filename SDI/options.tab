BEGIN_GUI_DESCR

	SHOW_PARAM STRING ITEM_NUM
	SHOW_PARAM STRING SUB_TYPE
	SHOW_PARAM STRING DOOR_ITEM_DESC
	SHOW_PARAM STRING FRAME_ITEM_DESC	
	
	IF FRAME_PROCESSED <> "PROCESSED"
		CHECKBOX_PARAM INTEGER HINGES "YES/NO"
		CHECKBOX_PARAM INTEGER CLOSER "YES/NO"
	END_IF
	! CHECKBOX_PARAM INTEGER WELDED_PREPS "YES/NO"
	
	IF OPENING_TYPE == 1 OR OPENING_TYPE == 3 
		CHECKBOX_PARAM INTEGER PRIMARY_LOCK "YES/NO"
		CHECKBOX_PARAM INTEGER SECONDARY_LOCK "YES/NO"
		IF SUB_TYPE == "P"
			CHECKBOX_PARAM INTEGER DUMMY_TRIM "YES/NO"
		END_IF
	END_IF
		
	IF OPENING_TYPE == 2 AND SUB_TYPE == "S"  AND FRAME_PROCESSED <> "PROCESSED"
		CHECKBOX_PARAM INTEGER PRIMARY_STRIKE "YES/NO"
		CHECKBOX_PARAM INTEGER SECONDARY_STRIKE "YES/NO"
	END_IF
		
	CHECKBOX_PARAM INTEGER PUSH_PULL "YES/NO"
	CHECKBOX_PARAM INTEGER HOLDER_STOP "YES/NO"
	
	IF CLOSER == 0
		CHECKBOX_PARAM INTEGER AUTO_OPERATOR "YES/NO"
	END_IF
	
	CHECKBOX_PARAM INTEGER COORDINATOR "YES/NO"
	
	IF SUB_TYPE == "S"
		CHECKBOX_PARAM INTEGER MAG_LOCK "YES/NO"
		IF MAG_LOCK > 0
			MAG_LOCK = 1
		END_IF
		CHECKBOX_PARAM INTEGER EPT "YES/NO"
		CHECKBOX_PARAM INTEGER DPS "YES/NO"
	END_IF
	
	IF SUB_TYPE == "P"
		! CHECKBOX_PARAM INTEGER ASTRAGAL "YES/NO"
		! IF ASTRAGAL == 1
			! CHECKBOX_PARAM INTEGER ASTRAGAL_INSTALLED "YES/NO"
		! END_IF
		
		RADIOBUTTON_PARAM INTEGER EPT "NONE" "LEFT - PAIR OR YES - SINGLE" "RIGHT - FOR PAIRS ONLY" "BOTH - FOR PAIRS ONLY" 
		RADIOBUTTON_PARAM INTEGER DPS "NONE" "LEFT - PAIR OR YES - SINGLE" "RIGHT - FOR PAIRS ONLY" "BOTH - FOR PAIRS ONLY"
		RADIOBUTTON_PARAM INTEGER MAG_LOCK "NONE" "LEFT - PAIR OR YES - SINGLE" "RIGHT - FOR PAIRS ONLY" "BOTH - FOR PAIRS ONLY" 
	END_IF
	
	IF SUB_TYPE == "P"
		RADIOBUTTON_PARAM INTEGER FLUSH_BOLTS "NO" "TOP" "BOTTOM" "TOP AND BOTTOM"
		
		IF FLUSH_BOLTS == 1 OR FLUSH_BOLTS == 3
			USER_INPUT_PARAM DOUBLE FLUSH_BOLT_CENTER_TOP
			RADIOBUTTON_PARAM INTEGER TOP_BOLT_TYPE "STD" "AUTOMATIC"
		END_IF
		IF FLUSH_BOLTS == 2 OR FLUSH_BOLTS == 3
			USER_INPUT_PARAM DOUBLE FLUSH_BOLT_CENTER_BOTTOM
			RADIOBUTTON_PARAM INTEGER BOTTOM_BOLT_TYPE "STD" "AUTOMATIC"
		END_IF
		
		IF FLUSH_BOLTS > 0
			USER_INPUT_PARAM STRING FLUSH_BOLT_MODEL
		END_IF
		
		RADIOBUTTON_PARAM INTEGER SURFACE_BOLTS "NO" "TOP" "BOTTOM" "TOP AND BOTTOM"
	ELSE
		FLUSH_BOLTS = 0
		SURFACE_BOLTS = 0
	END_IF
	
	IF OPENING_TYPE == 1 OR OPENING_TYPE == 3
		RADIOBUTTON_PARAM INTEGER AUTO_BOTTOM "NONE" "SURFACE" "MORTISE"
	END_IF
	IF OPENING_TYPE == 2 OR OPENING_TYPE == 3 AND FRAME_PROCESSED <> "PROCESSED"
		CHECKBOX_PARAM INTEGER CUSTOM_ANCHORS
	END_IF

	IF OPENING_TYPE == 2 OR OPENING_TYPE == 3 AND FRAME_PROCESSED <> "PROCESSED"
		CHECKBOX_PARAM INTEGER HEAD_STRIKE "YES/NO"
		CHECKBOX_PARAM INTEGER SILENCERS "YES/NO"
		IF SILENCERS == 1
			USER_INPUT_PARAM DOUBLE SILENCER_HOLE_DIA
		END_IF
	END_IF

	IF OPENING_TYPE == 1 OR OPENING_TYPE == 3
		CHECKBOX_PARAM INTEGER DOOR_EDGE_REINFORCEMENT "YES/NO"
		CHECKBOX_PARAM INTEGER KICK_PLATE "YES/NO"
	END_IF

	CHECKBOX_PARAM INTEGER HASP	
	IF HASP == 1
		USER_INPUT_PARAM DOUBLE HASP_CENTER
	END_IF
	
	CHECKBOX_PARAM INTEGER TUFLOCK	
	IF TUFLOCK == 1
		USER_INPUT_PARAM DOUBLE TUFLOCK_CENTER
	END_IF
	
	IF OPENING_TYPE == 2 OR OPENING_TYPE == 3 AND FRAME_PROCESSED <> "PROCESSED"
		CHECKBOX_PARAM INTEGER HOSPITAL_STOPS
	END_IF

	CHECKBOX_PARAM INTEGER PEEP
	IF PEEP == 1
		USER_INPUT_PARAM DOUBLE PEEP_CL
		USER_INPUT_PARAM DOUBLE PEEP_DIA
	END_IF
	
	RADIOBUTTON_PARAM INTEGER LEAD_LINED "NONE" "1/16" "1/8" "1/4"
	
END_GUI_DESCR

BEGIN_ASM_DESCR

	DECLARE_VARIABLE STRING ITEM_NUM
	DECLARE_VARIABLE STRING SUB_TYPE
	DECLARE_VARIABLE STRING OPENING_TYPE
	DECLARE_VARIABLE STRING DOOR_ITEM_DESC
	DECLARE_VARIABLE STRING FRAME_ITEM_DESC
	DECLARE_VARIABLE STRING HAND
	DECLARE_VARIABLE INTEGER PRIMARY_LOCK
	DECLARE_VARIABLE INTEGER DUMMY_TRIM
	DECLARE_VARIABLE INTEGER PRIMARY_STRIKE
	DECLARE_VARIABLE INTEGER SECONDARY_LOCK
	DECLARE_VARIABLE INTEGER SECONDARY_STRIKE
	DECLARE_VARIABLE INTEGER FLUSH_BOLTS
	DECLARE_VARIABLE DOUBLE FLUSH_BOLT_CENTER_TOP
	DECLARE_VARIABLE DOUBLE FLUSH_BOLT_CENTER_BOTTOM
	DECLARE_VARIABLE INTEGER MAG_LOCK
	DECLARE_VARIABLE INTEGER EPT
	DECLARE_VARIABLE INTEGER DPS
	DECLARE_VARIABLE INTEGER AUTO_BOTTOM
	DECLARE_VARIABLE INTEGER WELDED_PREPS
	DECLARE_VARIABLE INTEGER TOP_BOLT_TYPE
	DECLARE_VARIABLE INTEGER BOTTOM_BOLT_TYPE
	DECLARE_VARIABLE INTEGER CLOSER
	DECLARE_VARIABLE INTEGER HOLDER_STOP
	DECLARE_VARIABLE INTEGER AUTO_OPERATOR
	DECLARE_VARIABLE INTEGER KICK_PLATE
	DECLARE_VARIABLE INTEGER LEAD_LINED
	! DECLARE_VARIABLE INTEGER ASTRAGAL
	! DECLARE_VARIABLE INTEGER ASTRAGAL_INSTALLED
	DECLARE_VARIABLE INTEGER CUSTOM_ANCHORS	
	DECLARE_VARIABLE STRING  FLUSH_BOLT_MODEL
	DECLARE_VARIABLE INTEGER HASP
	DECLARE_VARIABLE DOUBLE HASP_CENTER
	DECLARE_VARIABLE DOUBLE SILENCER_HOLE_DIA	
	DECLARE_VARIABLE INTEGER TUFLOCK
	DECLARE_VARIABLE DOUBLE TUFLOCK_CENTER
	DECLARE_VARIABLE INTEGER HEAD_STRIKE
	DECLARE_VARIABLE INTEGER SURFACE_BOLTS
	DECLARE_VARIABLE INTEGER COORDINATOR
	DECLARE_VARIABLE INTEGER DOOR_EDGE_REINFORCEMENT
	DECLARE_VARIABLE INTEGER SILENCERS
	DECLARE_VARIABLE INTEGER HOSPITAL_STOPS
	DECLARE_VARIABLE INTEGER HINGES
	DECLARE_VARIABLE STRING  FRAME_PROCESSED
	DECLARE_VARIABLE INTEGER PEEP
	DECLARE_VARIABLE DOUBLE PEEP_CL
	DECLARE_VARIABLE DOUBLE PEEP_DIA
	
	IF FRAME_PROCESSED == "PROCESSED" AND PRIMARY_STRIKE == 1
		PRIMARY_LOCK = 1
	END_IF
! CONFIG_ELEM

	IF SUB_TYPE == "S"
		DUMMY_TRIM = 0
	END_IF
	
	IF SUB_TYPE == "S" AND DPS > 0
		DPS = 1
	END_IF

	IF SUB_TYPE == "S" AND EPT > 0
		EPT = 1
	END_IF
	
	IF OPENING_TYPE == 1 AND SUB_TYPE == "S"
		PRIMARY_STRIKE = 0
		SECONDARY_STRIKE = 0
	END_IF

	IF (OPENING_TYPE == 1 AND SUB_TYPE == "P") OR OPENING_TYPE == 3
		IF PRIMARY_LOCK == 0
			PRIMARY_STRIKE = 0
		ELSE
			PRIMARY_STRIKE = 1	
		END_IF
		IF SECONDARY_LOCK == 0
			SECONDARY_STRIKE = 0
		ELSE
			SECONDARY_STRIKE = 1
		END_IF
	END_IF

	IF OPENING_TYPE == 2
		PRIMARY_LOCK = 0
		SECONDARY_LOCK = 0
		DUMMY_TRIM = 0
	END_IF

END_ASM_DESCR



