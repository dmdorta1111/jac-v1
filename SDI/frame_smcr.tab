					GET_MDL_NAME SMCR SMCR_NAME
					INSERT_FAMINSTANCE SMCR SMCR_NAME + "_" + ITEM_NUM + "_" + itos(CLSR_INDEX)
					SET_FAMITEM_VALUE SMCR SMCR_NAME + "_" + ITEM_NUM + "_" + itos(CLSR_INDEX) "STOCKTYPE" 1
