						UDF_DIM	ROTY 0
						UDF_DIM	ROTZ 0
						UDF_DIM	LOCX ((JOW/2) - (SURFACE_CLOSER_LENGTH_/2))
						UDF_DIM	LOCY JOH + (FACEE/2)
						UDF_DIM	LOCZ (JD/2) - .25
						UDF_EXP_REF ATTACH CSYS 0
					END_CREATE_UDF

